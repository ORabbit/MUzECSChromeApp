{
  "name": "MUzECS",
  "version": "0.6",
  "manifest_version": 2,
  "description": "A chrome extenstion to upload compiled sketches to an arduino Leonardo",
  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },
  "permissions": [
    "serial",
    "usb",
    "http://*/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
